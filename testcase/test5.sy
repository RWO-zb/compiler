// test_05_expr.sy
// 目标：验证运算符优先级解析是否正确

int main() {
    int a = 1;
    int b = 2;
    int c = 3;
    int d = 4;
    
    // 优先级测试 1: 乘除高于加减
    int res1 = a + b * c;     // 1 + 6 = 7
    
    // 优先级测试 2: 括号
    int res2 = (a + b) * c;   // 3 * 3 = 9
    
    // 优先级测试 3: 关系运算高于相等性，相等性高于逻辑
    // 等价于: ((a < b) && (c > b)) || (d == 4)
    //        (True && True) || True -> 1
    int res3 = 0;
    if (a < b && c > b || d == 4) {
        res3 = 1;
    }
    
    // 复杂单目运算
    int res4 = -a * -b; // (-1) * (-2) = 2
    
    // 逻辑非
    int res5 = 0;
    if (!(a > b)) { // !False -> True
        res5 = 100;
    }

    return res1 + res2 + res3 + res4 + res5; 
    // 7 + 9 + 1 + 2 + 100 = 119
}