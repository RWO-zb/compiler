// test_11_syntax_stress.sy
// 目标：测试语法的健壮性，涵盖空语句、空块和复杂表达式

int main() {
    int a = 10;
    
    // 空语句
    ;;;
    
    // 表达式语句（合法但无副作用）
    a + 1;
    100;
    
    // 空块
    {
    }
    
    // 嵌套的空块与变量
    {
        int a = 20;
        {
            ;
        }
    }
    
    // 复杂的括号优先级
    int b = ((a + (2 * 5)) - 10) / 2; // (10 + 10 - 10) / 2 = 5
    
    if (a > 0) {
        if (b > 0) {
            return b; // 提前返回
        }
    }
    
    return 0; // 不应执行到这里
}